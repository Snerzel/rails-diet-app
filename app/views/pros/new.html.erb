<h1> Sign up as new pro! </h1>

<%= form_tag("/pros") do %>
    <p>
    <%= label_tag "Name:" %>
    <%= text_field_tag "name" %>
    </p>
    <p>
    <%= label_tag "Email:" %>
    <%= text_field_tag "email" %>
    </p>
    <p>
    <%= label_tag "Password:" %>
    <%= password_field_tag "password_digest" %>
    </p>
    <%= submit_tag "Sign up!" %>
    <% end %>