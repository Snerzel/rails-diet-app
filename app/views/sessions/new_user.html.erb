<h1> User Login </h1>

<%= form_tag('/sessions') do %>
    <p>
        <%= label_tag "Email:" %>
        <%= text_field_tag "email" %>
        </p>
        <p>
        <%= label_tag "Password:" %>
        <%= password_field_tag "password_digest" %>
        </p>
        <%= submit_tag "Login!" %>
        <% end %>